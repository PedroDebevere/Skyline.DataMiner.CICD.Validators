			<Name>TrapReceiver</Name>
			<Description>Trap Received</Description>
			<Type>dummy</Type>
			<SNMP>
				<Enabled>true</Enabled>
				<TrapOID setBindings="allBindingInfo" type="wildcard">*</TrapOID>
