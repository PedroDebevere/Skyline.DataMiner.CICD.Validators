			<Name>Response 100</Name>
		</Response>
	</Responses>

	<Pairs>
		<Pair id="100">
			<Name>Pair 100</Name>
			<Content>
				<Command>100</Command>
			</Content>
		</Pair>
	</Pairs>

	<Groups>
		<Group id="1">
			<Name>Other Group</Name>
			<Type>action</Type>
			<Content>
				<Action>1</Action>
			</Content>
		</Group>
		<Group id="100">
			<Name>Pair 100</Name>
			<Type>poll</Type>
			<Content>
				<Pair>100</Pair>
			</Content>
		</Group>
	</Groups>

	<Actions>
		<Action id="1">
			<Name>Other Action</Name>
		</Action>
	</Actions>
	
